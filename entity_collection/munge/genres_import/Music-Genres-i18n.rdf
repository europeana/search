    <skos:prefLabel xml:lang="en">Nerdcore</skos:prefLabel>
    <skos:prefLabel xml:lang="nl">Nerdcore</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Nerdcore</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Nerdcore rap</skos:prefLabel>
