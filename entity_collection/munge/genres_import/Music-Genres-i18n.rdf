    <skos:prefLabel xml:lang="bg">Полско диско</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Disco Polo</skos:prefLabel>
    <skos:prefLabel xml:lang="ko">디스코 폴로</skos:prefLabel>
    <skos:prefLabel xml:lang="no">Disco polo</skos:prefLabel>
    <skos:prefLabel xml:lang="fa">دیسکو پولو</skos:prefLabel>
    <skos:prefLabel xml:lang="pl">Disco polo</skos:prefLabel>
