        <skos:prefLabel xml:lang="id">Musik Afrika</skos:prefLabel>
        <skos:prefLabel xml:lang="it">Musica africana</skos:prefLabel>
        <skos:prefLabel xml:lang="no">Afrikansk musikk</skos:prefLabel>
