    <skos:prefLabel xml:lang="nl">Western Swing</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Western Swing</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Western Swing</skos:prefLabel>
    <skos:prefLabel xml:lang="pl">Western swing</skos:prefLabel>
