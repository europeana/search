  <skos:Concept rdf:about="http://data.europeana.eu/concept/music/genre/Festejo">
    <skos:prefLabel xml:lang="en">Festejo</skos:prefLabel>
    <skos:prefLabel xml:lang="nl">Festejo</skos:prefLabel>
    <skos:prefLabel xml:lang="es">Festejo</skos:prefLabel>
    <skos:closeMatch rdf:resource="http://rdf.freebase.com/ns/m.027z2gp"/>
    <skos:broader
      rdf:resource="http://data.europeana.eu/concept/music/genre/Latin_American_music_traditional"/>
    <skos:definition xml:lang="en">Festejo is a festive form of Peruvian music. It can be seen as a
      celebration of Perú's independence and the emancipation of slaves, or as an attempt to
      reinvent diaspora African music without reference to slavery. Composers of all races have
      contributed to the development of festejo repertoire. Its origins are in a competitive circle
      dance performed by men playing cajóns. Nowadays, people of all ages and races participate in a
      witty dance accompanying the festejo.</skos:definition>
    <skos:exactMatch xml:lang="en">http://dbpedia.org/resource/Festejo</skos:exactMatch>
    <skos:inScheme rdf:resource="http://data.europeana.eu/concept/music/genre/Music_Genre_Scheme"/>
    <skos:altLabel xml:lang="en">Festejo</skos:altLabel>
  </skos:Concept>

