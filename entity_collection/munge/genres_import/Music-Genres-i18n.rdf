  <skos:Concept rdf:about="http://data.europeana.eu/concept/music/genre/Arabesque_(Turkish_music)">
    <skos:prefLabel xml:lang="en">Arabesque</skos:prefLabel>
    <skos:prefLabel xml:lang="ar">أرابيسك</skos:prefLabel>
