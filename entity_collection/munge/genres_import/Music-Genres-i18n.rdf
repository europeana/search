    <skos:prefLabel xml:lang="fr">Skweee</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Skweee</skos:prefLabel>
    <skos:prefLabel xml:lang="fa">اسکویی</skos:prefLabel>
    <skos:prefLabel xml:lang="pl">Skweee</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Скви</skos:prefLabel>
