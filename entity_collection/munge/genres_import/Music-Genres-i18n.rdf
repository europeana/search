  <skos:Concept rdf:about="http://data.europeana.eu/concept/music/genre/Sinawi">
    <skos:prefLabel xml:lang="en">Sinawe</skos:prefLabel>
    <skos:prefLabel xml:lang="id">Sinawi</skos:prefLabel>
