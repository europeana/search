    <skos:prefLabel xml:lang="nl">Screwed &amp; chopped</skos:prefLabel>
    <skos:prefLabel xml:lang="fi">Chopped and Screwed</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Chopped and screwed</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Screw</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Chopped and screwed</skos:prefLabel>
