  <skos:Concept rdf:about="http://data.europeana.eu/concept/music/genre/Country_rap">
    <skos:prefLabel xml:lang="en">Country rap</skos:prefLabel>
    <skos:prefLabel xml:lang="fr">Country rap</skos:prefLabel>
    <skos:prefLabel xml:lang="it">Country rap</skos:prefLabel>
    <skos:prefLabel xml:lang="pt">Country rap</skos:prefLabel>
    <skos:prefLabel xml:lang="ru">Кантри-рэп</skos:prefLabel>
    <skos:prefLabel xml:lang="sk">Country rap</skos:prefLabel>
